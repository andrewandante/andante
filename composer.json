{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "^4.0.0",
		"silverstripe/framework": "4.0.0-alpha4",
		"silverstripe/reports": "^4.0.0",
		"silverstripe/siteconfig": "^4.0.0",
		"silverstripe/blog": "dev-FIX/view-permissions-on-posts",
		"silverstripe/comments": "master",
		"colymba/gridfield-bulk-editing-tools": "^3.0",
		"jonom/silverstripe-betternavigator": "dev-UPGRADE/ss4-compatiblilty",
		"silverstripe/asset-admin": "^1.0.0-alpha4"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:andrewandante/silverstripe-blog.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:andrewandante/silverstripe-betternavigator.git"
		}
	],
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.5.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
