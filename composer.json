{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "^4.0.0",
		"silverstripe/framework": "^4.0.0",
		"silverstripe/reports": "^4.0.0",
		"silverstripe/siteconfig": "^4.0.0",
		"silverstripe/blog": "dev-ss4",
		"silverstripe/comments": "dev-feature/ss4-compat",
		"colymba/gridfield-bulk-editing-tools": "dev-feature/ss4-compat as 2.1.x-dev",
		"jonom/silverstripe-betternavigator": "dev-UPGRADE/ss4-compatiblilty",
		"silverstripe/asset-admin": "^1.0@alpha"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:robbieaverill/silverstripe-blog.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:robbieaverill/silverstripe-comments.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:robbieaverill/GridFieldBulkEditingTools.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:andrewandante/silverstripe-betternavigator.git"
		}
	],
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.5.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
